<%= form_with model: @doctor, local: true do |form| %>
    <p>
         First Name:
        <%= form.text_field :first_name %>
    </p>

    <p>
         Last Name:
        <%= form.text_field :last_name %>
    </p>

    <p>
        zip code:
        <%= form.text_field :zip_code %>
    </p>

    <p>
        specialty:
        <%= form.text_field :specialty %>
    </p>
    
    <%= form.submit %>
    <% end %> 